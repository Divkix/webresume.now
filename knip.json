{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["worker.ts", "open-next.config.ts", "scripts/**/*.ts"],
  "project": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "hooks/**/*.{ts,tsx}",
    "lib/**/*.{ts,tsx}"
  ],
  "ignore": ["app/globals.css"],
  "ignoreBinaries": [],
  "ignoreDependencies": ["cloudflare", "tailwindcss", "tw-animate-css"],
  "ignoreExportsUsedInFile": true
}
