{
  "name": "webresume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:worker": "bunx opennextjs-cloudflare build",
    "start": "next start",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next .open-next",
    "ci": "bun run typecheck && bun run lint && bun run build",
    "preview": "bunx opennextjs-cloudflare build && bunx opennextjs-cloudflare preview",
    "deploy": "bunx opennextjs-cloudflare build && bunx wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "wrangler d1 migrations apply webresume-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply webresume-db --remote",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && bun run db:migrate",
    "db:studio": "drizzle-kit studio --port 4984"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/s3-request-presigner": "^3.943.0",
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "^1.14.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "better-auth": "^1.4.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.0",
    "lucide-react": "^0.555.0",
    "next": "^15.5.7",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "replicate": "^1.4.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cloudflare/workers-types": "^4.20251205.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.53.0"
  }
}
