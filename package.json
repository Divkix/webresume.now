{
  "name": "webresume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:worker": "bunx opennextjs-cloudflare build",
    "start": "next start",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next .open-next",
    "ci": "bun run type-check && bun run lint && bun run build",
    "preview": "bunx opennextjs-cloudflare build && bunx opennextjs-cloudflare preview",
    "deploy": "bunx opennextjs-cloudflare build && bunx wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "wrangler d1 migrations apply webresume-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply webresume-db --remote",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && bun run db:migrate",
    "db:studio": "drizzle-kit studio --port 4984",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv lib/cloudflare-env.d.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@opennextjs/cloudflare": "^1.15.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@types/bun": "^1.3.6",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "next": "^16.1.4",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "unpdf": "^1.4.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.60.0"
  }
}
