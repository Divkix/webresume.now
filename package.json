{
  "name": "clickfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:worker": "bunx opennextjs-cloudflare build",
    "start": "next start",
    "lint": "biome check .",
    "fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next .open-next",
    "test": "vitest run",
    "test:watch": "vitest",
    "ci": "bun run type-check && bun run lint && bun run test && bun run build",
    "preview": "bunx opennextjs-cloudflare build && bunx opennextjs-cloudflare preview",
    "deploy": "bunx opennextjs-cloudflare build && bunx wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "wrangler d1 migrations apply clickfolio-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply clickfolio-db --remote",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && bun run db:migrate",
    "db:studio": "drizzle-kit studio --port 4984",
    "analyze": "ANALYZE=true next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv lib/cloudflare-env.d.ts",
    "generate:favicons": "bun run scripts/generate-favicons.ts",
    "db:seed:local": "bun run scripts/seed-local.ts",
    "db:reset:local": "bun run db:reset && bun run db:seed:local"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.27",
    "@hookform/resolvers": "^5.2.2",
    "@neoconfetti/react": "^1.0.0",
    "@opennextjs/cloudflare": "^1.16.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "ai": "^6.0.70",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "ofetch": "^1.5.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "unpdf": "^1.4.0",
    "uplot": "^1.6.32",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@next/bundle-analyzer": "^16.1.6",
    "@noble/hashes": "^1.7.1",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.1",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "to-ico": "^1.1.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.61.1"
  }
}
